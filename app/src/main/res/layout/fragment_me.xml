<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/me_background">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/indi_head_day"/>



    <ImageView
        android:id="@+id/me_sign_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/sign_up"
        android:layout_centerInParent="true"/>

    <TextView
        android:id="@+id/me_zhuCe_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/me_sign_iv"
        android:layout_marginTop="@dimen/margin_30dip"
        android:textColor="@color/black"
        android:text="@string/resgiter"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/me_yonghuxieyi_tv"
        android:layout_marginBottom="@dimen/margin_20dip"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/me_signWeiX_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_20dip"
            android:layout_marginRight="@dimen/margin_10dip"
            android:src="@mipmap/bubble_wechat"/>
        <View
            android:layout_width="@dimen/margin_10dip"
            android:layout_height="1dip"
            android:layout_weight="1"></View>
        <ImageView
            android:id="@+id/me_signXinL_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_10dip"
            android:layout_marginRight="@dimen/margin_10dip"
            android:src="@mipmap/bubble_weibo"/>
        <View
            android:layout_width="@dimen/margin_10dip"
            android:layout_height="1dip"
            android:layout_weight="1"></View>
        <ImageView
            android:id="@+id/me_signQQ_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_10dip"
            android:layout_marginRight="@dimen/margin_20dip"
            android:src="@mipmap/bubble_qq"/>

    </LinearLayout>

    <TextView
        android:id="@+id/me_yonghuxieyi_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/margin_10dip"
        android:text="@string/instorcate"/>



    <ImageView
        android:id="@+id/me_setting_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/center_setting"
        android:layout_marginTop="@dimen/margin_20dip"
        android:layout_marginLeft="@dimen/margin_20dip"/>

</RelativeLayout>